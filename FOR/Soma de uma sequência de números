numbers_amount = int(input()) #Quantidade de numeros
total = 0
if(numbers_amount <= 10000 and numbers_amount >= 0): #a quantidade de numeros n pode ser negativa e nem maior que 10000
    for i in range(0,numbers_amount): #Aqui iremos iterar numbers_amount vezes
        number = int(input()) #Pedimos um numero
        total += number #Guardamo-o na variavel total e somamos com o total.
print("%d\n"%total)
